<% collection.each do |mixtape| %>
   <div class="tapes">
      <h1><%= link_to mixtape.name, mixtape_path(mixtape), class: 'mixtape_name', id: "mixtape_#{mixtape.id}" %></h1>
      <p>(<%= mixtape.songs.length %>) Songs about: <%= mixtape.about %></p>
      <p>Posted <%= distance_of_time_in_words(DateTime.now, mixtape.created_at) %> ago</p>
      <p>Edited <%= distance_of_time_in_words(DateTime.now, mixtape.updated_at) %> ago</p>
      <%= link_to 'Edit Mixtape Details', edit_mixtape_path(mixtape) %>
   </div>
<% end %>
