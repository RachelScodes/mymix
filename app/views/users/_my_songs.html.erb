<% collection.each do |song| %>
   <div class="songs">
      <p class="song-info"><span class="italic"><%= song.title %></span> by <%= song.artist %></p>
      <%= render partial: 'songs/play_button', :locals => { :song => song }%>
      <p class="links">
         <%= link_to "View Song Details", song_path(song) %>
      </p>
      <p>Posted <%= distance_of_time_in_words(DateTime.now, song.created_at) %> ago</p>
      <p>Edited <%= distance_of_time_in_words(DateTime.now, song.updated_at) %> ago</p>
   </div>
<% end %>
