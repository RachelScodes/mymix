<ul id="mixtapes">
   <% @mixtapes.each do |mixtape| %>
     <li>
       <%= link_to mixtape.name, mixtape_path(mixtape), class: 'mixtape_name', id: "mixtape_#{mixtape.id}" %>
       <p>(<%= mixtape.songs.length %>) Songs about: <%= mixtape.about %></p>
       <p>Posted <%= distance_of_time_in_words(DateTime.now, mixtape.created_at) %> ago</p>
     </li>
   <% end %>
</ul>
