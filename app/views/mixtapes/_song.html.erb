<div class="song">
  <p class="song-info"><span class="italic"><%= song.title %></span> by <%= song.artist %>
  <span class="song-links"><%= link_to "Play", song.src_url %> | <%= link_to "Song Detail", edit_song_path(params[:id]) %> ...Posted <%= distance_of_time_in_words(DateTime.now, song.created_at) %> ago</p>

</div>
